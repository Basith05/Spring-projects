<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/aop
    http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
    http://www.springframework.org/schema/tx
    http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<context:component-scan
		base-package="com.example.demo"></context:component-scan>
		
	<import resource="classpath:spring/helloWorld-config.xml"/>
	
	<bean id="greetingController"
		class="com.example.demo.controller.GreetingController">
		<property name="helloWorldService" ref="helloWorldServiceDefault"></property>
		<property name="helloWorldServiceTamil" ref="helloWorldServiceTamil"></property>
		<property name="helloWorldServiceEng2" ref="eng2"></property>
	</bean>
		


</beans>
